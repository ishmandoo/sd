{
  "name": "student",
  "base": "PersistedModel",
  "idInjection": true,
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "status": {
      "type": "string",
      "required": true
    },
    "last_action_date": {
      "type": "date",
      "required": true
    },
    "pin": {
      "type": "string",
      "required": false
    }
  },
  "validations": [],
  "relations": {
    "classes": {
      "type": "hasMany",
      "model": "class",
      "foreignKey": "classId",
      "through": "seat"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    }
  ],
  "methods": []
}
